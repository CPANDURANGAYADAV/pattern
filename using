# pattern
i=1
j=1
x=int(input("Enter number of rows required :))
for i in range(1,x+1):
   for j in range(1,i+1):
      print(i,end=" ")
   print()
# checking the number is polyndrum
rev=0
num=int(input("Enter number : "))
while(num):
  rev=rev*10 + (num%10)
  num=num/10
if(num==rev):
   print("this number is polyndrum")
else:
  print("this number is not polyndrum")
   
   
 # check the number is armstrong number
 sum=0
 num=int(input("Enter the number : "))
 while(num):
   dig=num%10
   sum=sum+dig*dig*dig
   num=num%10
if(num==sum):
   print("This number is armstrong number")
else:
   print("this is not armstrong number")
 
